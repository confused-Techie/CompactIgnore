{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "eqeqeq": "off",
    "no-use-before-define": "off",
    "quotes": ["error", "double"],
    "arrow-parens": "off",
    "no-console": "off",
    "arrow-spacing": "on",
    "block-scoped-var": "on",
    "complexity": ["error", 15]
  },
  "reportUnusedDisableDirectives": true
}
